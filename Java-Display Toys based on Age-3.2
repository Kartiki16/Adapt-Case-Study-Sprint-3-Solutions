public  String getToy(int age)
{
    String toy;
    switch(age){
        case 1:
            toy="Stickle Bricks";
            break;
        case 2:
            toy="Robot Dog";
            break;
        case 3:
            toy="Magic 8 Ball";
            break;
        case 4:
            toy="Juggling Clubs";
            break;
        case 5:
            toy="Chutes and Ladders";
            break;
        default:
        toy="no toys available";
    }
    return toy;
}

	
    public static void main( String[] args )
	{
